{
    "version": 2,
    "builds": [
        {
            "src":"car_Rental_booking/wsgi.py",
            "use":"@vercel/python",
            "config":{"maxLambdaSize": "15mb", "runtime": "Python 3.10.0"}
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/static-builder",
            "config":{
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*)",
            "dest":"car_Rental_booking/wsgi.py"
        }
    ]
}